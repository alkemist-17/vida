
let N = 10_000_000
let data = list(N, 0)

for i = 0, len(data) {
    data[i] = i
}

let binarySearch = fun xs, key {
    var lo = 0
    var hi = len(xs)-1
    while lo <= hi {
        var mid = lo + (hi-lo) / 2
        if key < xs[mid] {
            hi = mid - 1
        } else if key > xs[mid] {
            lo = mid + 1
        } else {
            ret mid
        }
    }
}

assert(binarySearch(data, 7)==7)
assert(binarySearch(data, 176_565)==176_565)
assert(binarySearch(data, -1)==nil)
assert(binarySearch(data, N+1)==nil)
assert(binarySearch(data, 567_901)==567_901)

