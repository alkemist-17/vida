
var x = 0xFFFFFFFF
var y = 16

print(format("Bits: %032b", x))
print(format("Bits: %032b", y))

print(format("BOR : %032b", x | y))
print(format("BAND: %032b", x & y))
print(format("BXOR: %032b", x ^ y))
print(format("BSHL: %032b", x << y))
print(format("BSHR: %032b", x >> y))
print(format("BNOT: %032b", ~x))
print(format("BNOT: %032b", ~y))

