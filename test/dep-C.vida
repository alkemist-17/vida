
let math = load("std/math")

let script = {}

script.Pi = math.pi

script.Tau = math.tau

assert(
    script.Pi == 3.141592653589793 or 
    script.Pi <= 3.141592653589793 or
    script.Pi >= 3.141592653589793
)

assert(
    script.Tau == 6.283185307179586 or
    script.Tau <= 6.283185307179586 or
    script.Tau >= 6.283185307179586
)

export script