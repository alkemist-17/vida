assert(bool("true") == true)
assert(bool("false") == false)
assert(not bool(""))

assert(float("1.42") == 1.42)
assert(float("1e-5") == 1e-5)
assert(float("-1e5") == -1e5)
assert(float("0.0") == 0.0)
assert(float("-0.0") == -0.0)

assert(int("-42") == -42)
assert(int("42") == 42)
assert(int("42.25") == nil)
assert(int("0xFF") == 0xFF)
assert(int("0b1010") == 0b1010)

assert(str(90) == "90")
assert(str(true) == "true")
assert(str(false) == "false")
assert(str(nil) == "nil")
assert(str(45.899) == "45.899")
