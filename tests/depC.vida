
let math = load("math")

let module = {}

module.Pi = math.pi

module.Tau = math.tau

assert(
    module.Pi == 3.141592653589793 or 
    module.Pi <= 3.141592653589793 or
    module.Pi >= 3.141592653589793
)

assert(
    module.Tau == 6.283185307179586 or
    module.Tau <= 6.283185307179586 or
    module.Tau >= 6.283185307179586
)

export module