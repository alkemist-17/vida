assert(toBool("true") == true)
assert(toBool("false") == false)
assert(not toBool(""))

assert(toFloat("1.42") == 1.42)
assert(toFloat("1e-5") == 1e-5)
assert(toFloat("-1e5") == -1e5)
assert(toFloat("0.0") == 0.0)
assert(toFloat("-0.0") == -0.0)

assert(toInt("-42") == -42)
assert(toInt("42") == 42)
assert(toInt("42.25") == nil)
assert(toInt("0xFF") == 0xFF)
assert(toInt("0b1010") == 0b1010)

assert(toNil("nil") == nil)
assert(toNil("") == nil)

